[
  {
    "name": "Lock screen",
    "exec": "swaylock -f --clock --fade-in 2 --screenshots --effect-blur 7x5 --indicator",
    "icon": "/home/maxh/.config/nwg-launchers/nwgbar/lock.svg"
  },
  {
    "name": "Logout",
    "exec": "swaymsg exit",
    "icon": "/home/maxh/.config/nwg-launchers/nwgbar/logout.svg"
  },
  {
    "name": "Sleep",
    "exec": "swaylock -f --clock --screenshots --effect-blur 7x5 --indicator && systemctl hybrid-sleep",
    "icon": "/home/maxh/.config/nwg-launchers/nwgbar/power-sleep.svg"
  },
  {
    "name": "Reboot",
    "exec": "systemctl reboot",
    "icon": "/home/maxh/.config/nwg-launchers/nwgbar/restart.svg"
  },
  {
    "name": "Shutdown",
    "exec": "systemctl -i poweroff",
    "icon": "/home/maxh/.config/nwg-launchers/nwgbar/power.svg"
  }
]
